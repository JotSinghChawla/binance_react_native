{
  "expo": {
    "name": "binance_react_native",
    "slug": "binance_react_native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.jotsingh.binance_react_native"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "A marketplace for Buying & Selling Cryptocurrencies !!",
    "githubUrl": "https://github.com/JotSinghChawla/binance_react_native"
  }
}
